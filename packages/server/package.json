{
    "name": "@sea/server",
    "private": true,
    "type": "module",
    "version": "0.6.0",
    "main": "./src/index.js",
    "exports": {
        ".": {
            "types": "./src/router/index.ts"
        }
    },
    "scripts": {
        "start": "nodemon ./src/index.ts",
        "watch:tsc": "tsc -w"
    },
    "dependencies": {
        "@fastify/cors": "^8.4.1",
        "@fastify/env": "^4.3.0",
        "@fastify/middie": "^8.3.0",
        "@fastify/one-line-logger": "^1.2.0",
        "@fastify/static": "^6.12.0",
        "@fastify/websocket": "^8.2.0",
        "@trpc/server": "^10.44.1",
        "close-with-grace": "^1.2.0",
        "fastify": "^4.24.3",
        "http-proxy-middleware": "3.0.0-beta.1",
        "smol-toml": "^1.1.3",
        "superjson": "^2.2.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@swc/core": "^1.3.99",
        "@types/connect": "^3.4.38",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.1"
    },
    "nodemonConfig": {
        "watch": [
            "./tsconfig.json",
            "./src/**/*",
            ".env"
        ],
        "ext": "ts,js",
        "execMap": {
            "ts": "node --import=./typescript-hooks.mjs"
        }
    }
}