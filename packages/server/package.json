{
  "name": "@sea/server",
  "private": true,
  "type": "module",
  "version": "0.6.0",
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/router/index.ts"
    }
  },
  "scripts": {
    "start": "nodemon ./src/index.ts",
    "watch:tsc": "tsc -w"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.1",
    "@fastify/env": "^4.3.0",
    "@fastify/middie": "^8.3.0",
    "@fastify/one-line-logger": "^1.2.0",
    "@fastify/static": "^6.12.0",
    "@fastify/websocket": "^8.2.0",
    "@trpc/server": "^10.44.1",
    "close-with-grace": "^1.2.0",
    "fastify": "^4.24.3",
    "http-proxy-middleware": "3.0.0-beta.1",
    "rotating-file-stream": "^3.1.1",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.99",
    "@types/connect": "^3.4.38",
    "@yao-pkg/pkg": "^5.11.4",
    "esbuild": "0.20.1",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.3.0",
    "ts-node": "^10.9.1"
  },
  "nodemonConfig": {
    "watch": [
      "./tsconfig.json",
      "./src/**/*",
      ".env"
    ],
    "ext": "ts,js",
    "execMap": {
      "ts": "node --import=./typescript-hooks.mjs"
    }
  },
  "bin": "./src/index.cjs",
  "pkg": {
    "scripts": [
      "../../node_modules/thread-stream/**/*.js"
    ],
    "targets": [
      "node20-win-x64"
    ]
  }
}
