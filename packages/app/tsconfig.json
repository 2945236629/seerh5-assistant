{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "tsBuildInfoFile": "../../.tsbuildinfo/app.tsbuildinfo",
        "moduleResolution": "bundler",
        "allowImportingTsExtensions": true,
        "isolatedModules": true,
        "resolveJsonModule": true,
        "noEmit": true,
        "jsx": "react",
        "paths": {
            "@sa-app/*": [
                "./src/*"
            ],
            "@mui/styled-engine": [
                "../../node_modules/@mui/styled-engine-sc"
            ],
        },
        "composite": true,
        "types": [
            "vite/client"
        ]
    },
    "include": [
        "./src",
        "../../types"
    ],
    "references": [
        {
            "path": "../core"
        }
    ]
}